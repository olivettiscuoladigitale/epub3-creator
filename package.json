{
  "name": "epub-creator",
  "version": "1.0.0",
  "description": "Create epub from html or text",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "./node_modules/.bin/webpack-dev-server --progress --colors --watch --no-minimize",
    "build": "./node_modules/.bin/webpack --config webpack.config.build.js --progress --colors; ./node_modules/.bin/webpack --config webpack.config.build.js --progress --colors -p",
    "doc": "./node_modules/.bin/typedoc  --exclude ./node_modules/@types/jszip/index.d --out doc/ src/"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/filesaver": "0.0.30",
    "@types/jszip": "0.0.31",
    "file-saver": "^1.3.3",
    "jszip": "^3.1.3",
    "jszip-utils": "0.0.2",
    "tslint": "^3.15.1",
    "tslint-loader": "^2.1.5"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.18.0",
    "source-map-loader": "^0.1.5",
    "ts-loader": "^1.1.0",
    "typedoc": "^0.5.1",
    "typedoc-webpack-plugin": "^1.1.3",
    "typescript": "^2.0.8",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  }
}
